{
    "version": 2,
    "name": "kingdom-collective-website",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/privacy",
            "destination": "/privacy"
        },
        {
            "source": "/terms",
            "destination": "/terms"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                },
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "origin-when-cross-origin"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "camera=(), microphone=(), geolocation=()"
                }
            ]
        }
    ],
    "env": {
        "NEXT_PUBLIC_APP_URL": "https://kingdomcollective.pro",
        "NEXT_PUBLIC_API_URL": "https://api.kingdomcollective.pro",
        "NEXT_PUBLIC_STORE_URL": "https://desitotrh.com"
    }
}