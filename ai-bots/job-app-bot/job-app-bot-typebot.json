{
  "version": "6",
  "id": "job-app-bot",
  "name": "Job Application Bot",
  "events": [
    {
      "id": "evt1",
      "type": "start",
      "outgoingEdgeId": "edge1",
      "graphCoordinates": {
        "x": 0,
        "y": 0
      }
    }
  ],
  "groups": [
    {
      "id": "group1",
      "title": "Main Flow",
      "graphCoordinates": {
        "x": 0,
        "y": 0
      },
      "blocks": [
        {
          "id": "welcome",
          "type": "text",
          "label": "Welcome",
          "content": {
            "type": "text",
            "content": "Welcome to our job application process! Let's get started. ðŸ’¼"
          }
        },
        {
          "id": "position-interest",
          "type": "choice input",
          "label": "Position Interest",
          "question": {
            "type": "text",
            "content": "What type of position are you interested in?"
          },
          "choices": [
            {
              "id": "opt1",
              "label": "Full-time"
            },
            {
              "id": "opt2",
              "label": "Part-time"
            },
            {
              "id": "opt3",
              "label": "Contract"
            },
            {
              "id": "opt4",
              "label": "Internship"
            }
          ]
        },
        {
          "id": "name-question",
          "type": "text",
          "label": "Name Question",
          "content": {
            "type": "text",
            "content": "Let's start with your basic information."
          }
        },
        {
          "id": "name-input",
          "type": "text input",
          "label": "Name Input",
          "question": {
            "type": "text",
            "content": "What's your full name?"
          },
          "variable": "applicant_name"
        },
        {
          "id": "email-question",
          "type": "text",
          "label": "Email Question",
          "content": {
            "type": "text",
            "content": "Great! Now I need your contact information."
          }
        },
        {
          "id": "email-input",
          "type": "email input",
          "label": "Email Input",
          "question": {
            "type": "text",
            "content": "What's your email address?"
          },
          "variable": "applicant_email"
        },
        {
          "id": "experience-question",
          "type": "text",
          "label": "Experience Question",
          "content": {
            "type": "text",
            "content": "Tell me about your relevant experience."
          }
        },
        {
          "id": "experience-input",
          "type": "text input",
          "label": "Experience Input",
          "question": {
            "type": "text",
            "content": "Describe your relevant experience..."
          },
          "variable": "applicant_experience"
        },
        {
          "id": "application-confirmation",
          "type": "text",
          "label": "Application Confirmation",
          "content": {
            "type": "text",
            "content": "Thank you, {{applicant_name}}! Your application has been submitted. We'll review it and contact you within 5-7 business days."
          }
        }
      ]
    }
  ],
  "edges": [
    {
      "id": "edge1",
      "from": {
        "blockId": "evt1",
        "itemId": "",
        "pathId": ""
      },
      "to": {
        "groupId": "group1",
        "blockId": "welcome"
      }
    }
  ],
  "variables": [],
  "theme": {
    "general": {
      "font": "Inter",
      "background": {
        "type": "Color",
        "content": "#FFFFFF"
      }
    }
  },
  "settings": {
    "enableUserInput": true,
    "autoStart": true,
    "showRestartButton": true,
    "typingDelay": 30
  }
}